%body.sign
  .header-box
    .header-box__sign-up
      %h1.header-box__sign-up__logo
        =link_to image_tag("logo.png",width:"185",height:"49"), root_path  
      %nav.header-box__sign-up__box
        %ol
          %li.header-box__sign-up__box__text
            会員情報
            .header-box__sign-up__box__round--start
          %li.header-box__sign-up__box__text{ id: "first" }
            お届け先住所入力
            .header-box__sign-up__box__round--green-last
          %li.header-box__sign-up__box__text--active{ id: "end" }
            完了
            .header-box__sign-up__box__round__last
%body.sign
  .main
    .main__box
      .main__box__container__one
        %h2.main__box__h2 登録が完了しました
        %br
      .main__box__container__two
        .back
          = link_to("トップへ戻る", root_path, {class: "back-home"})

= render partial: './simpleFooter'